{
  "name": "web-client",
  "version": "1.0.0",
  "description": "New npm-gui client based on VueJS",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --history-api-fallback --inline",
    "build": "webpack -p",
    "test:unit": "karma start test/unit/karma.conf.js --single-run",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "node node_modules/stylelint/dist/cli.js 'components/*/**.vue' *.scss --syntax scss",
    "lint:js": "node node_modules/eslint/bin/eslint 'components/*/**.vue' 'components/*/**.js'",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run lint",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/npm-gui-base/web-client.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/npm-gui-base/web-client/issues"
  },
  "homepage": "https://github.com/npm-gui-base/web-client#readme",
  "devDependencies": {
    "webpack-dev-server": "^1.16.2"
  },
  "engines": {
    "node": ">= 0.10.0",
    "npm": ">= 1.0.0"
  },
  "dependencies": {
    "webpack": "^1.13.3",
    "normalize.css": "^5.0.0",
    "babel-loader": "^6.2.7",
    "vue": "v2.0.8",
    "url-loader": "^0.5.7",
    "vue-resource": "^1.0.3",
    "vue-loader": "^9.7.0",
    "autoprefixer": "^6.5.1",
    "stylelint-webpack-plugin": "^0.4.0",
    "eslint": "^3.9.1",
    "eslint-loader": "^1.6.1",
    "sass-loader": "^4.0.2",
    "eslint-config-airbnb-base": "^5.0.3",
    "style-loader": "^0.13.1",
    "html-webpack-plugin": "^2.24.1",
    "sass-flex-mixin": "^1.0.3",
    "stylelint-config-standard": "^15.0.0",
    "file-loader": "^0.9.0",
    "babel-preset-es2015": "^6.18.0",
    "node-sass": "^3.11.1",
    "babel-preset-stage-2": "^6.18.0",
    "eslint-plugin-html": "^1.5.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-eslint": "^7.1.0",
    "stylelint-processor-html": "^1.0.0",
    "babel-polyfill": "^6.16.0",
    "css-loader": "^0.25.0",
    "eslint-plugin-import": "^1.16.0",
    "babel-core": "^6.18.2",
    "vue-router": "^2.0.3"
  }
}
